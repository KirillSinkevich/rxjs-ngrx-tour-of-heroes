{
  "name": "angular-io-example",
  "version": "0.0.0",
  "description": "Example project from an angular.io guide.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint-ng": "ng lint",
    "lint": "eslint .",
    "prettier": "prettier '**/*.{js,ts,html,md,scss,less,json}'",
    "style-lint": "stylelint '**/*.scss'",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npx lint --fix"
    ],
    "**/*": "prettier --check",
    "**/*.{scss}": "style-lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0-rc.0",
    "@angular/cdk": "^15.2.3",
    "@angular/common": "^15.2.0-rc.0",
    "@angular/compiler": "^15.2.0-rc.0",
    "@angular/core": "^15.2.0-rc.0",
    "@angular/forms": "^15.2.0-rc.0",
    "@angular/material": "^15.2.3",
    "@angular/platform-browser": "^15.2.0-rc.0",
    "@angular/platform-browser-dynamic": "^15.2.0-rc.0",
    "@angular/router": "^15.2.0-rc.0",
    "@ngrx/component-store": "^15.4.0",
    "angular-in-memory-web-api": "~0.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0-rc.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.0-rc.0",
    "@angular/compiler-cli": "^15.2.0-rc.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.35",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-node": "~10.9.0",
    "typescript": "^4.9.5"
  }
}
